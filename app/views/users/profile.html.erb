<h2 class= "page-title" >Welcome, <%= @user.name.capitalize %>!</h2>

<%= link_to "Edit Profile", edit_user_path %>
<table>
  <tr>
    <th>
      Username:
    </th>
    <td>
      <%= @user.username %>
    </td>
  </tr>
  <tr>
    <th>
      Email:
    </th>
    <td>
      <%= @user.email %>
    </td>
  </tr>
  <tr>
    <th>
      Storename:
    </th>
    <td>
      <%= @user.storename %>
    </td>
  </tr>
</table>
